<!-- 子カテゴリがあれば表示 -->
<% if (category.children) { %>
    <section class="sec-maker-parts-frame-category">
        <ul class="mod-selectCategory">
            <li class="mod-selectCategory-category">あ</li>
            <li class="mod-selectCategory-category">い</li>
            <li class="mod-selectCategory-category">う</li>
            <li class="mod-selectCategory-category">え</li>
            <li class="mod-selectCategory-category">お</li>
        </ul>
    </section>
<% } %>

<!-- パーツ選択するエリア -->
<section class="sec-maker-parts-frame-parts">
    <% let url = '/assets/images/pages/maker/parts/' %>
    <%# if category.children %>
        <%# const childUrl = url + category.name + '/' %>
        <%# for (const child in category.children) { %>
            <%# include('./selectParts', {category: child, url: childUrl}) %>
        <%# } %>
    <%# } else { %>
        <%- include('./selectParts', {category: category, url: url}) %>
    <%# } %>
</section>

