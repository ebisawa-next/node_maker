    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "babel-loader": "^8.3.0",
    "babel-watch": "^7.0.0",
    "browser-sync": "^2.23.3",
    "concurrently": "^3.5.1",
    "core-js": "^3.2.1",
    "css-loader": "^6.7.3",
    "express": "^4.17.1",
    "mini-css-extract-plugin": "^2.7.2",
    "multer": "^1.4.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.14.7",
